{
  "name": "@welcome-ui/repo",
  "version": "1.1.1",
  "main": "dist/welcome-ui.cjs.js",
  "module": "dist/welcome-ui.es.js",
  "description": "Customizable design system with react • styled-components • styled-system and reakit.",
  "scripts": {
    "start": "yarn docs:dev",
    "icons:optimize": "svgo --folder=icons/_assets --config='{ \"plugins\": [ { \"cleanupIDs\": false } ] }' --indent=0 --multipass --quiet",
    "icons:collect": "node -r esm scripts/icons.js",
    "pre-build": "yarn icons:optimize && yarn icons:collect",
    "build": "node scripts/build.js",
    "release:publish": "yarn lerna publish --conventional-commits",
    "release:prerelease": "yarn build && yarn lerna publish --conventional-prerelease --pre-dist-tag next --yes",
    "release": "yarn build && yarn release:publish && yarn docs:build",
    "lint": "yarn lint:mdx & yarn lint:js && yarn lint:css",
    "lint:js": "eslint packages icons scripts --max-warnings 0 --fix",
    "lint:css": "stylelint 'packages/**/styles.js'",
    "lint:mdx": "prettier --parser mdx --write 'packages/**/*.mdx'",
    "test": "jest ./packages",
    "clean": "lerna clean",
    "docs:dev": "next",
    "docs:build": "next build"
  },
  "private": true,
  "workspaces": [
    "packages/**/*",
    "../packages/**/*",
    "icons",
    "icons/**/*",
    "../icons/**/*",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WTTJ/welcome-ui.git"
  },
  "keywords": [
    "design-system",
    "react",
    "reakit",
    "styled-components",
    "styled-system",
    "ui-library",
    "ui",
    "ux",
    "jungle",
    "welcome",
    "WTTJ"
  ],
  "author": "WTTJ <tech@wttj.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WTTJ/welcome-ui/issues"
  },
  "devDependencies": {
    "@babel/cli": "=7.4.3",
    "@babel/core": "=7.4.3",
    "@babel/plugin-proposal-class-properties": "=7.4.0",
    "@babel/plugin-proposal-optional-chaining": "=7.9.0",
    "@babel/plugin-transform-modules-commonjs": "=7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "=7.4.3",
    "@babel/preset-react": "=7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@emotion/core": "^10.0.9",
    "@rollup/plugin-json": "^4.0.2",
    "@testing-library/dom": "=6.7.0",
    "@testing-library/react": "^8.0.1",
    "@testing-library/user-event": "=7.0.1",
    "@xstyled/styled-components": "^1.17.0",
    "@xstyled/system": "^1.17.0",
    "babel-eslint": "=10.0.3",
    "babel-jest": "=24.7.1",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-styled-components": "=1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "=0.4.24",
    "colors": "^1.4.0",
    "conventional-github-releaser": "^3.1.3",
    "css-loader": "=2.1.1",
    "date-fns": "=2.8.1",
    "depcheck": "=0.9.1",
    "eslint": "=6.5.1",
    "eslint-config-prettier": "=6.4.0",
    "eslint-config-react-app": "=5.0.2",
    "eslint-plugin-flowtype": "=4.3.0",
    "eslint-plugin-import": "=2.18.2",
    "eslint-plugin-jsx-a11y": "=6.2.3",
    "eslint-plugin-prettier": "=3.1.1",
    "eslint-plugin-react": "=7.16.0",
    "eslint-plugin-react-hooks": "=2.1.2",
    "eslint-plugin-sort-destructure-keys": "=1.3.3",
    "esm": "^3.2.25",
    "final-form": "=4.13.1",
    "gatsby": "^2.15.34",
    "gatsby-plugin-eslint": "=2.0.8",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-styled-components": "=3.1.13",
    "husky": "=3.1.0",
    "jest": "=24.7.1",
    "jest-dom": "^3.5.0",
    "jest-styled-components": "=7.0.0",
    "lerna": "^3.19.0",
    "lerna-update-wizard": "^0.17.7",
    "lodash.capitalize": "=4.2.1",
    "lodash.difference": "^4.5.0",
    "lodash.kebabcase": "^4.1.1",
    "minimist": "=1.2.0",
    "prettier": "=1.18.2",
    "prop-types": "^15.7.2",
