    "react-dom": "^16.10.2",
    "react-final-form": "=6.0.1",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.1",
    "react-router-dom": "=5.1.2",
    "rollup": "=1.12.4",
    "rollup-plugin-babel": "=4.3.2",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-postcss": "=2.0.3",
    "rollup-plugin-size-snapshot": "^0.9.0",
    "standard-version": "^6.0.1",
    "style-loader": "=0.23.1",
    "styled-components": "^4.0.0 || ^5.0.0",
    "stylelint": "=12.0.0",
    "stylelint-config-recommended": "=3.0.0",
    "stylelint-config-styled-components": "=0.1.1",
    "stylelint-processor-styled-components": "=1.9.0",
    "svgo": "=1.2.2",
    "webfonts-generator": "^0.4.0",
    "wttj-config-front": "=1.0.1",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "next": "^9.4.0"
  },
  "peerDependencies": {
    "@xstyled/styled-components": "^1.15.1",
    "@xstyled/system": "^1.15.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-components": "^4.0.0 || ^5.0.0"
  },
  "resolutions": {
    "docz/**/webpack": "4.41.1",
    "styled-components": "5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn icons:optimize && yarn icons:collect && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "http://welcome-ui.com"
}
